{
  "name": "@one-gogh/server",
  "version": "1.0.0",
  "private": true,
  "description": "One Gogh system NodeJS web server",
  "main": "index.js",
  "scripts": {
    "generate": "sequelize migration:generate --name $@",
    "generate:seed": "sequelize seed:generate --name $@",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "dev": "nodemon index.js",
    "start": "NODE_ENV=production node index.js",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo",
    "migrate:test:undo:all": "NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watchAll --coverage"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ],
    "delay": "100"
  },
  "author": "Badousai",
  "dependencies": {
    "@one-gogh/contract": "*",
    "aws-sdk": "^2.690.0",
    "axios": "^0.21.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "ethereumjs-util": "^7.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-useragent": "^1.0.15",
    "fast-csv": "^4.3.6",
    "hashids": "^2.2.1",
    "helmet": "^4.1.0",
    "http-terminator": "^2.0.3",
    "moment": "^2.26.0",
    "multer": "^1.4.2",
    "nocache": "^2.1.0",
    "node-cron": "^3.0.0",
    "paseto": "^3.0.1",
    "pg": "^8.4.1",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "request-ip": "^2.1.3",
    "require-dir": "^1.2.0",
    "sequelize": "^6.3.5",
    "streamifier": "0.1.1",
    "tedious": "^11.0.9",
    "umzug": "^3.0.0-beta.15",
    "uuid-validate": "0.0.3",
    "web3": "^1.6.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "nodemon": "^2.0.4",
    "jest": "^27.4.5",
    "supertest": "^6.1.6",
    "sequelize-cli": "^6.2.0",
    "faker": "^5.5.3"
  }
}
